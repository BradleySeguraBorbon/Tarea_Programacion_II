<?xml version="1.0" encoding="UTF-8"?>

<?import io.github.palexdev.materialfx.controls.MFXTextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cr.ac.una.tarea_bradleysegura_noemimurillo_programacion_ii.controller.CardPrintController">
   <children>
      <VBox alignment="CENTER" layoutX="369.0" layoutY="128.0" prefHeight="245.0" prefWidth="262.0" spacing="20.0">
         <children>
            <Label text="Ingrese el folio del cliente" />
            <MFXTextField fx:id="txtInFolio" floatingText="Folio" maxWidth="1.7976931348623157E308" />
            <Button fx:id="btnInputFolio" mnemonicParsing="false" onAction="#browseUser" text="Ingresar" />
            <MFXTextField fx:id="txtNameUser" allowEdit="false" floatingText="Nombre" maxWidth="1.7976931348623157E308" />
            <Button fx:id="btnPrint" mnemonicParsing="false" onAction="#printPDF" text="Generar carnet" />
         </children>
      </VBox>
   </children>
</AnchorPane>
